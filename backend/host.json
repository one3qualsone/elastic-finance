{
    "version": "2.0",
    "extensionBundle": {
      "id": "Microsoft.Azure.Functions.ExtensionBundle",
      "version": "[4.*, 5.0.0)"
    },
    "extensions": {
      "http": {
        "routePrefix": "",
        "cors": {
          "allowedOrigins": [
            "https://elasticfinancestorage.z6.web.core.windows.net",
            "http://localhost:3000"
